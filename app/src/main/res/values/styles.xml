<resources xmlns:android="http://schemas.android.com/apk/res/android">

    <style name="Base.ApartSlideTheme.Button.Input" parent="">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:minHeight">50dp</item>
        <item name="android:padding">@dimen/spacing_large</item>
        <item name="android:textColor">@color/colors_white_text</item>
        <item name="progressColor">@color/white</item>
    </style>

    <style name="ApartSlideTheme.Button.Input" parent="Base.ApartSlideTheme.Button.Input"/>

    <style name="Base.AppTheme.Widget.Button" parent="@android:style/Widget.Holo.Button.Borderless">
        <item name="android:textAppearance">@style/TextAppearance.Regular</item>
        <item name="android:textSize">@dimen/font_normal</item>
    </style>

    <style name="AppTheme.Widget.Button" parent="Widget.AppCompat.Button.Borderless">
        <item name="android:background">@drawable/btn_subaccent</item>
        <item name="android:textColor">@color/colors_white_text</item>
    </style>

    <style name="AppTheme.Widget.Button.Auth" parent="">
        <item name="android:background">@drawable/bg_auth_button</item>
        <item name="android:minWidth">280dp</item>
        <item name="android:minHeight">50dp</item>
        <item name="android:textColorHint">@color/white_50</item>
        <item name="android:textSize">@dimen/font_micro</item>
        <item name="android:textColor">@color/colors_auth_button_text</item>
        <item name="android:padding">@dimen/spacing_large</item>
    </style>


    <style name="AppTheme.Widget.Button.Outline">
        <item name="android:background">@drawable/btn_subaccent_border</item>
        <item name="android:textColor">@color/subAccentColor</item>
    </style>

    <style name="AppTheme.Widget.Button.Outline.Dark">
        <item name="android:background">@drawable/btn_gray_7_border</item>
        <item name="android:textColor">@color/colors_gray_7_text</item>
    </style>

    <style name="AppTheme.Widget.Button.Outline.Fill">
        <item name="android:background">@drawable/btn_subaccent_border_fill</item>
    </style>

    <style name="AppTheme.Widget.Button.Confirm">
        <item name="android:text">@string/label.confirm</item>
    </style>

    <style name="Divider">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">@dimen/divider_size</item>
        <item name="android:background">@color/divider</item>
    </style>

    <style name="Divider.ZummaStore">
        <item name="android:layout_height">1dp</item>
    </style>

    <style name="Divider.Vertical">
        <item name="android:layout_width">@dimen/divider_size</item>
        <item name="android:layout_height">match_parent</item>
    </style>

    <style name="Divider.Bold">
        <item name="android:layout_height">@dimen/divider_bold_height</item>
    </style>

    <style name="Divider.Review" parent="">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:text">|</item>
        <item name="android:textColor">@color/gray_a</item>
    </style>

    <style name="Divider.Spacing" parent="Divider">
        <item name="android:layout_marginTop">2dp</item>
        <item name="android:layout_marginBottom">2dp</item>
    </style>

    <style name="Caution.Title" parent="">
        <item name="android:textColor">@color/green</item>
    </style>

    <style name="Caution.Content" parent="">
        <item name="android:textColor">@color/black</item>
    </style>

    <style name="ProgressBar.Button" parent="">
        <item name="android:indeterminate">true</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:visibility">gone</item>
        <item name="android:maxWidth">24dp</item>
        <item name="android:maxHeight">24dp</item>
        <item name="android:minWidth">24dp</item>
        <item name="android:minHeight">24dp</item>
    </style>

    <style name="Tag"></style>

    <style name="Tag.MarketItem">
        <item name="android:paddingRight">6dp</item>
        <item name="android:paddingLeft">6dp</item>
        <item name="android:paddingTop">2dp</item>
        <item name="android:paddingBottom">2dp</item>
        <item name="android:textColor">@color/white</item>
        <item name="android:textSize">@dimen/font_micro</item>
    </style>

    <style name="Divider.Guide">
        <item name="android:layout_marginTop">@dimen/spacing_large</item>
        <item name="android:layout_marginBottom">@dimen/spacing_large</item>
    </style>

    <style name="Guide"></style>

    <style name="Guide.Title">
        <item name="android:textColor">@color/gray_7</item>
        <item name="android:textSize">@dimen/font_micro</item>
    </style>

    <style name="Guide.Content">
        <item name="android:layout_marginBottom">@dimen/spacing_micro</item>
        <item name="android:lineSpacingMultiplier">1.5</item>
        <item name="android:textColor">@color/black</item>
        <item name="android:textSize">@dimen/font_micro</item>
    </style>

    <style name="Guide.Content.Warning">
        <item name="android:textColor">@color/pink</item>
    </style>

    <style name="Shadow">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">2.5dp</item>
    </style>

    <style name="Shadow.TopDown">
        <item name="android:background">@drawable/img_shadow_topdown</item>
    </style>

    <style name="Shadow.BottomUp">
        <item name="android:background">@drawable/img_shadow_bottomup</item>
    </style>

    <style name="TabLayout" parent="Widget.Design.TabLayout">
        <item name="android:background">@color/white</item>
        <item name="tabBackground">?attr/selectableItemBackground</item>
        <item name="tabIndicatorColor">@color/subAccentColor</item>
        <item name="elevation">@dimen/elevation</item>
    </style>

    <style name="TextAppearance.Order.Title" parent="TextAppearance.AppCompat">
        <item name="android:paddingBottom">@dimen/spacing_small</item>
        <item name="android:paddingTop">@dimen/spacing_small</item>
        <item name="android:background">@color/gray_e</item>
        <item name="android:paddingLeft">@dimen/spacing_large</item>
        <item name="android:paddingRight">@dimen/spacing_large</item>
    </style>

    <style name="TextAppearance.Order.Radio" parent="TextAppearance.AppCompat">
        <item name="android:paddingLeft">@dimen/spacing_small</item>
        <item name="android:paddingRight">0dp</item>
        <item name="android:textColor">@color/colors_radio</item>
    </style>

    <style name="SettingsTextAppearance.Title" parent="">
        <item name="android:textSize">@dimen/spacing_small</item>
        <item name="android:layout_marginBottom">@dimen/spacing_smaller</item>
        <item name="android:layout_marginLeft">@dimen/spacing_smaller</item>
        <item name="android:layout_marginTop">@dimen/spacing_large</item>
        <item name="android:textColor">@color/gray_7</item>
    </style>

    <style name="Apartment.Container" parent="">
        <item name="android:paddingLeft">@dimen/spacing_large</item>
        <item name="android:paddingRight">@dimen/spacing_large</item>
    </style>

    <style name="DropDownListView" parent="">
        <item name="android:layout_margin">10dp</item>
        <item name="divider">@android:color/transparent</item>
        <item name="android:background">@color/white</item>
        <item name="android:divider">@android:color/transparent</item>
        <item name="android:dividerHeight">0dp</item>
        <item name="android:listSelector">?selectableItemBackground</item>
    </style>

    <style name="TextAppearance.Light" parent="android:TextAppearance">
    </style>

    <style name="TextAppearance.Regular" parent="android:TextAppearance">
        <item name="android:textSize">@dimen/font_normal</item>
        <item name="android:textColor">@color/black</item>
    </style>

    <style name="TextAppearance.AppBar" parent="TextAppearance.Regular">
        <item name="android:textSize">@dimen/font_large</item>
    </style>

    <style name="TextAppearance.Auth.Message" parent="">
        <item name="android:gravity">center</item>
        <item name="android:textSize">@dimen/font_micro</item>
        <item name="android:textColor">@color/white</item>
    </style>

    <style name="AppTheme.Widget.Button.ATEvent" parent="@style/Widget.AppCompat.Button.Borderless">
        <item name="android:background">@drawable/btn_at_event</item>
        <item name="android:textColor">@color/colors_white_text</item>
    </style>

    <style name="AppTheme.ActionBar" parent="ThemeOverlay.AppCompat.ActionBar">
        <item name="elevation">@dimen/elevation</item>
        <item name="android:textSize">@dimen/font_large</item>
    </style>

    <style name="ShareDialogItem" parent="Widget.AppCompat.Button">
        <item name="android:minWidth">0dp</item>
        <item name="android:minHeight">0dp</item>
        <item name="android:gravity">start|center_vertical</item>
        <item name="android:padding">@dimen/spacing_normal</item>
        <item name="android:drawablePadding">@dimen/spacing_normal</item>
        <item name="android:background">?attr/selectableItemBackground</item>
    </style>

    <style name="Animation.Dropdown" parent="">
        <item name="android:windowEnterAnimation">@anim/expanding</item>
        <item name="android:windowExitAnimation">@anim/collapsing</item>
    </style>

    <style name="AppTheme.Widget.EditText" parent="">
        <item name="android:background">@drawable/bg_edittext_normal</item>
        <item name="android:gravity">center_vertical</item>
        <item name="android:minHeight">@dimen/edittext_height</item>
        <item name="android:padding">@dimen/spacing_large</item>
    </style>

    <style name="AppTheme.Widget.EditText.Auth" parent="">
        <item name="android:background">@drawable/bg_auth_edit</item>
        <item name="android:gravity">center</item>
        <item name="android:minWidth">280dp</item>
        <item name="android:minHeight">50dp</item>
        <item name="android:lines">1</item>
        <item name="android:maxLines">1</item>
        <item name="android:textColorHint">@color/white_50</item>
        <item name="android:textSize">@dimen/font_micro</item>
        <item name="android:textColor">@color/colors_auth_edit_text</item>
        <item name="android:padding">@dimen/spacing_large</item>
    </style>

    <style name="AppTheme.Widget.EditText.SingleLine">
        <item name="android:inputType">text</item>
        <item name="android:lines">1</item>
        <item name="android:maxLines">1</item>
    </style>

    <style name="AppTheme.Logo">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_gravity">center_horizontal</item>
        <item name="android:layout_marginBottom">80dp</item>
        <item name="android:layout_marginTop">60dp</item>
        <item name="android:src">@drawable/img_logo_init</item>
    </style>

    <style name="AppTheme.CircularProgressBar" parent="CircularProgressBar">
        <item name="android:layout_gravity">center</item>
        <item name="android:layout_width">@dimen/progress_size_normal</item>
        <item name="android:layout_height">@dimen/progress_size_normal</item>
        <item name="cpb_color">@color/deep_yellow</item>
    </style>

    <style name="AppTheme.CircularProgressBar.ProgressButton">
        <item name="android:layout_width">@dimen/progress_size_micro</item>
        <item name="android:layout_height">@dimen/progress_size_micro</item>
        <item name="cpb_color">@color/white</item>
    </style>

    <style name="AppTheme.CircularProgressBar.Small">
        <item name="android:layout_width">@dimen/progress_size_small</item>
        <item name="android:layout_height">@dimen/progress_size_small</item>
        <item name="cpb_stroke_width">2dp</item>
    </style>

    <style name="AppTheme.CircularProgressBar.Micro">
        <item name="android:layout_width">@dimen/progress_size_micro</item>
        <item name="android:layout_height">@dimen/progress_size_micro</item>
        <item name="cpb_stroke_width">1dp</item>
    </style>

    <style name="CollapsingToolbarLayoutExpandedNoTitleStyle" parent="AppTheme">
        <item name="android:textColor">@android:color/transparent</item>
    </style>

    <style name="GroupTitle">
        <item name="android:textSize">@dimen/font_normal</item>
        <item name="android:textColor">@color/gray_9</item>
        <item name="android:textAppearance">@style/TextAppearance.Regular</item>
        <item name="android:layout_gravity">center_vertical</item>
    </style>

    <style name="Widget.AppTheme.Mypage" parent=""/>
    <style name="Widget.AppTheme.Preference" parent=""/>

    <style name="Widget.AppTheme.Mypage.Container">
        <item name="android:minHeight">100dp</item>
        <item name="android:background">@color/white</item>
        <item name="android:foreground">?selectableItemBackground</item>
    </style>

    <style name="Widget.AppTheme.Preference.Container">
        <item name="android:minHeight">60dp</item>
        <item name="android:background">?selectableItemBackground</item>
        <item name="android:paddingRight">@dimen/spacing_large</item>
        <item name="android:paddingLeft">@dimen/spacing_large</item>
        <item name="android:gravity">center_vertical</item>
    </style>

    <style name="TextAppearance.AppTheme.Preference.Title" parent="">
        <item name="android:textColor">@color/gray_a</item>
        <item name="android:paddingTop">@dimen/spacing_normal</item>
        <item name="android:paddingBottom">@dimen/spacing_normal</item>
        <item name="android:paddingRight">@dimen/spacing_large</item>
        <item name="android:paddingLeft">@dimen/spacing_large</item>
        <item name="android:gravity">center_vertical</item>
        <item name="android:textSize">11sp</item>
    </style>

    <style name="Widget.AppTheme.Mypage.Item">
        <item name="android:layout_marginTop">22dp</item>
        <item name="android:layout_gravity">center_horizontal</item>
        <item name="android:drawablePadding">10dp</item>
        <item name="android:gravity">center</item>
        <item name="android:textSize">@dimen/font_mini</item>
        <item name="android:lineSpacingExtra">2dp</item>
    </style>

    <style name="Widget.AppTheme.Mypage.Divider">
        <item name="android:layout_width">@dimen/divider_size</item>
        <item name="android:layout_height">match_parent</item>
        <item name="android:background">@color/divider</item>
    </style>

    <style name="Widget.AppTheme.FlatButton"
           parent="@style/Widget.AppCompat.Button.Borderless">
    </style>

</resources>